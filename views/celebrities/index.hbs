<ul class="list-group-flush celebIndex">
{{#each celebrities}}
<div class='unit'>
  <li class="list-group-item"><a href="/celebrities/{{ _id }}">{{name}}</a></li>
  <li class="list-group-item"><a href="/celebrities/{{ _id }}/edit" class="btn btn-outline-success"> edit </a></li>
  <li class="list-group-item"><form action="/celebrities/{{ _id }}/delete" method="POST"><button class="btn btn-outline-danger"> delete </button></form></li>
</div>
{{/each}}
