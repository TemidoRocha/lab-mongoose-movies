<ul class="list-group-flush celebIndex">
  {{#each movies}}
  <div class='unit'>
    <li class="list-group-item"><a href="/movies/{{ _id }}">{{ title }}</a></li>
    <li class="list-group-item"><a href="/movies/{{ _id }}/edit" class="btn btn-outline-success"> edit </a></li>
    <li class="list-group-item"><form action="/movies/{{ _id }}/delete" method="POST"><button class="btn btn-outline-danger"> delete </button></form></li>
  </div>
  {{/each}}
</ul>

